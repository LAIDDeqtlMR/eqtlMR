

BiocManager::install(c("VariantAnnotation", "remote"))
remotes::install_github(c("MRCIEU/gwasvcf","MRCIEU/gwasglue", "MRCIEU/ieugwasr"))
library(VariantAnnotation)
library(gwasvcf)
library(gwasglue)
library(ieugwasr)
library(remote)

curl -O http://fileserve.mrcieu.ac.uk/ld/1kg.v3.tgz

library(gwasvcf)
set_bcftools('/path/to/bcftools')
set_plink('/path/to/plink')

library(gwasvcf)
remotes::install_github('mrcieu/genetics.binaRies')  #위에 두 툴이 설치되는 툴
library(set_plink)
library(bcftools)

./set_plink
./bcftools

tar jxvf bcftools-1.18.tar.bz2

zcat ../eqtlgen_exposure_dat_snps_5kb. cut -f 9 | tail -n +2 sort -u | less

cut -f 9 ../eqtlgen | tail -n +2 | sort -u | split -l 50 --numeric-suffixes=100 -a  /dev/stdin exposures.

for I in {00..11}; do echo $I; done

tail -n +2 ../eqtl | sort -k 10 -n -k 11 -n | cut -f 9 |uniq | split -l 50 --numeric-suffixes=100 -a /dev/stdin exposures.


